        <JDBCSampler guiclass="TestBeanGUI" testclass="JDBCSampler" testname="Query-MDM-agency-supplier-enabled" enabled="true">
          <stringProp name="dataSource">dbconnMDM</stringProp>
          <stringProp name="query">SELECT o1.name AS agencyName,
  o1.PUBLIC_ID as agencyMOID,
  sa.CREATEDBY,
  o2.public_Id as supplierMOID,
  o2.name as supplierName,
  case when sa.IS_DELETED =1 then &apos;false&apos;
  when sa.IS_DELETED =0 then &apos;true&apos;
  end as enabled
FROM SEC_SELLER_AGENCY_ASSOCIATION sa
JOIN SEC_ORGANISATION o1
ON sa.HOLDINGCOMPANY_ID = o1.PARENT_ORGANISATION_ID
JOIN SEC_ORGANISATION O2
on sa.MEDIAOWNER_ID = o2.PARENT_ORGANISATION_ID
join sec_users s
on s.id = sa.createdby
where rownum&lt;=100
and s.STATUS_ID =1
GROUP BY o1.NAME,
  o1.PUBLIC_ID,
  sa.CREATEDBY,
  o2.public_Id ,
    o2.name,
    sa.IS_DELETED
ORDER BY o1.NAME,
  o1.PUBLIC_ID,
  sa.CREATEDBY,
  o2.public_Id,
    o2.name,
    sa.IS_DELETED
  </stringProp>
          <stringProp name="queryArguments"></stringProp>
